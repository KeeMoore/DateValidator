@model DojoSurveyVal.Models.SurveyViewModel

<!DOCTYPE html>
<html>

<head>
    <title>Submitted Info</title>
    <link rel="stylesheet" href="~/css2/site.css">
</head>

<body>
    <div class="info-box">
        <h1>Submitted Info</h1>
        <p>Name: @Model.Name</p>
        <p>Dojo Location: @Model.Location</p>
        <p>Favorite Language: @Model.FavoriteLanguage</p>
        <p>Comment: @Model.Comment</p>
        <p>Survey Date: @Model.SurveyDate</p>
        <form action="/Home/Index" method="get">
            <button type="submit">Go Back</button>
        </form>
    </div>
</body>

</html>
